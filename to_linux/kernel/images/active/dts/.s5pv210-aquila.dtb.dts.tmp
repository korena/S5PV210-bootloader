# 633 "arch/arm/boot/dts/s5pv210.dtsi" 2
# 19 "arch/arm/boot/dts/s5pv210-aquila.dts" 2

/ {
 model = "Samsung Aquila based on S5PC110";
 compatible = "samsung,aquila", "samsung,s5pv210";

 aliases {
  i2c3 = &i2c_pmic;
 };

 chosen {
  bootargs = "console=ttySAC2,115200n8 root=/dev/mmcblk1p5 rw rootwait ignore_loglevel earlyprintk";
 };

 memory {
  device_type = "memory";
  reg = <0x30000000 0x05000000
   0x40000000 0x18000000>;
 };

 regulators {
  compatible = "simple-bus";
  #address-cells = <1>;
  #size-cells = <0>;

  vtf_reg: fixed-regulator@0 {
   compatible = "regulator-fixed";
   reg = <0>;
   regulator-name = "V_TF_2.8V";
   regulator-min-microvolt = <2800000>;
   regulator-max-microvolt = <2800000>;
   gpio = <&mp05 4 0>;
   enable-active-high;
  };

  pda_reg: fixed-regulator@1 {
   compatible = "regulator-fixed";
   regulator-name = "VCC_1.8V_PDA";
   regulator-min-microvolt = <1800000>;
   regulator-max-microvolt = <1800000>;
   reg = <1>;
  };

  bat_reg: fixed-regulator@2 {
   compatible = "regulator-fixed";
   regulator-name = "V_BAT";
   regulator-min-microvolt = <3700000>;
   regulator-max-microvolt = <3700000>;
   reg = <2>;
  };
 };

 i2c_pmic: i2c-pmic {
  compatible = "i2c-gpio";
  gpios = <&gpj4 0 0>,
   <&gpj4 3 0>;
  i2c-gpio,delay-us = <2>;
  #address-cells = <1>;
  #size-cells = <0>;

  pmic@66 {
   compatible = "national,lp3974";
   reg = <0x66>;

